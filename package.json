    "test": "NODE_ENV=test jest --config ./jest.config.js",
    "test:windows": "SET NODE_ENV=test && jest --config ./jest.config.js",
    "stats": "webpack --config webpack.prod.js --json > stats.json && webpack-bundle-analyzer stats.json"
